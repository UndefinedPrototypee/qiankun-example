{
  "name": "qiankun-example",
  "version": "0.0.1",
  "description": "qiankun demo",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "install": "npm-run-all --serial install:*",
    "install:main": "cd main && npm i",
    "install:sub-angular": "cd sub-angular && npm i",
    "install:sub-html": "cd sub-html && npm i",
    "install:sub-react": "cd sub-react && npm i",
    "install:sub-vue": "cd sub-vue && npm i",
    "install:sub-vue-doctor": "cd sub-vue-doctor && npm i",
    "install:sub-vue-nurse": "cd sub-vue-nurse && npm i",
    "install:sub-vue-hospital": "cd sub-vue-hospital && npm i",
    "install:sub-vue-patient": "cd sub-vue-patient && npm i",
    "install:sub-order": "cd sub-order && npm i",
    "install:sub-vue3": "cd sub-vue3 && npm i",
    "install:sub-umi": "cd sub-umi && npm i",

    "start": "npm-run-all --parallel start:*",
    "start:main": "cd main && npm start",
    "start:sub-angular": "cd sub-angular && npm start",
    "start:sub-html": "cd sub-html && npm start",
    "start:sub-react": "cd sub-react && npm start",
    "start:sub-vue": "cd sub-vue && npm start",
    "start:sub-vue-doctor": "cd sub-vue-doctor && npm start",
    "start:sub-vue-nurse": "cd sub-vue-nurse && npm start",
    "start:sub-vue-hospital": "cd sub-vue-hospital && npm start",
    "start:sub-vue-patient": "cd sub-vue-patient && npm start",
    "start:sub-order": "cd sub-order && npm start",
    "start:sub-vue3": "cd sub-vue3 && npm start",
    "start:sub-umi": "cd sub-umi && npm start",

    "build": "npm-run-all build:*",
    "build:main": "cd main && npm run build",
    "build:sub-angular": "cd sub-angular && npm run build",
    "build-sub-html": "cd sub-html && npm run build",
    "build:sub-react": "cd sub-react && npm run build",
    "build:sub-vue": "cd sub-vue && npm run build",
    "build:sub-vue-doctor": "cd sub-vue-doctor && npm run build",
    "build:sub-vue-nurse": "cd sub-vue-nurse && npm run build",
    "build:sub-vue-hospital": "cd sub-vue-hospital && npm run build",
    "build:sub-vue-patient": "cd sub-vue-patient && npm run build",
    "build:sub-order": "cd sub-order && npm run build",
    "build:sub-vue3": "cd sub-vue3 && npm run build",
    "build:sub-umi": "cd sub-umi && npm run build",

    "build-and-deploy": "npm-run-all build:* && bash ./scripts/bundle.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@fengxianqi.github.com:fengxianqi/qiankun-example.git"
  },
  "keywords": [
    "qiankun",
    "qiankun-example",
    "demo"
  ],
  "author": "fengxianqi",
  "license": "MIT"
}
